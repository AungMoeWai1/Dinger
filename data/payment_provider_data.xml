<?xml version="1.0" encoding="utf-8" ?>
<odoo noupdate="1">

    <record id="payment_provider_dinger" model="payment.provider">
        <field name="name">Dinger</field>
        <field name="code">dinger</field>
        <field
                name="image_128"
                type="base64"
                file="payment_dinger/static/description/icon.png"
        />
        <field name="module_id" ref="base.module_payment_dinger"/>
        <field name="redirect_form_view_id" ref="redirect_form"/>
        <field
                name="payment_method_ids"
                eval="[(6, 0, [
                   ref('payment_dinger.payment_method_dinger_ayapay'),
                   ref('payment_dinger.payment_method_dinger_cbpay'),
                   ref('payment_dinger.payment_method_dinger_citizenspay'),
                   ref('payment_dinger.payment_method_dinger_jcb'),
                   ref('payment_dinger.payment_method_dinger_kbz'),
                   ref('payment_dinger.payment_method_dinger_kpay'),
                   ref('payment_dinger.payment_method_dinger_mab_mobile_banking'),
                   ref('payment_dinger.payment_method_dinger_master'),
                   ref('payment_dinger.payment_method_dinger_m_pite_san'),
                   ref('payment_dinger.payment_method_dinger_mpt_pay'),
                   ref('payment_dinger.payment_method_dinger_mytelpay'),
                   ref('payment_dinger.payment_method_dinger_okdollar'),
                   ref('payment_dinger.payment_method_dinger_onepay'),
                   ref('payment_dinger.payment_method_dinger_sai_sai_pay'),
                   ref('payment_dinger.payment_method_dinger_true_money'),
                   ref('payment_dinger.payment_method_dinger_uab_pay'),
                   ref('payment_dinger.payment_method_dinger_visa'),
                   ref('payment_dinger.payment_method_dinger_wave_pay'),
               ])]"
        />
    </record>

</odoo>
